{
  "version": 2,
  "builds": [
    {
      "src": "api/index-lite.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "10mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index-lite.py"
    }
  ],
  "functions": {
    "api/index-lite.py": {
      "memory": 512,
      "maxDuration": 10
    }
  }
} 