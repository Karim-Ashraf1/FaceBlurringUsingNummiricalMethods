{
  "version": 2,
  "builds": [
    {
      "src": "api/index-lite.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "10mb",
        "memory": 512,
        "maxDuration": 10,
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index-lite.py"
    }
  ]
} 