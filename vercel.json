{
  "version": 2,
  "builds": [
    {
      "src": "api/index-minimal.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "5mb",
        "memory": 256,
        "maxDuration": 10,
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index-minimal.py"
    }
  ]
} 